  ask x0   = (Checker.bind (Equal a.x0 b.x0))
  ask x1   = (Checker.bind (Equal a.x1 b.x1))
  (Checker.done (Bool.and ctid (Bool.and x0 x1)))

// Ct3 equality
(Equal (Ct3 a.ctid a.orig a.x0 a.x1 a.x2) (Ct3 b.ctid b.orig b.x0 b.x1 b.x2)) =
